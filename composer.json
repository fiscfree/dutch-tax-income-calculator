{
    "name": "fiscfree/dutch-tax-income-calculator",
    "version": "1.0.0",
    "description": "Dutch Tax Income Calculator for Laravel - Calculate Dutch income tax, social security contributions, and tax credits",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim van Heugten",
            "email": "tim.vanheugten@fiscfree.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "DutchTaxCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DutchTaxCalculator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DutchTaxCalculator\\DutchTaxCalculatorServiceProvider"
            ],
            "aliases": {
                "DutchTaxCalculator": "DutchTaxCalculator\\Facades\\DutchTaxCalculator"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
